package CollisionProjectile
	import Collisions
	import Projectile
	import Vectors
	import Entity
	
	public abstract class CollisionProjectile extends Projectile
		
		construct( vec3 pos, real radius, real angle, string sfxpath )
			super( pos, radius, angle, sfxpath )
			collisionEntities.add(this)
			
			
		override function update()
			super.update()
			for Entity e in collisionEntities
				if e.owner != owner and checkCollision(e)
					onCollision(e)
					e.onCollision(this)

endpackage